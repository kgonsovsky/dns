<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <!-- URL Rewrite Rule -->
        <rewrite>
            <rules>
                <rule name="AlwaysRedirectToIndexJS" stopProcessing="true">
                    <match url=".*" />
                    <action type="Rewrite" url="/index.js" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>